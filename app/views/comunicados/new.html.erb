<turbo-frame id="cobra-modal">
  <div class="cobra-modal-background"></div>
  <div data-controller="cobra-modal"
    data-action="
      turbo:submit-end -> cobra-modal#hide
      keydown.esc -> cobra-modal#hide
    "
    role="dialog"
    tabindex="-1"
    class="modal-transition-in">
      <h2>Criar comunicado</h2>

      <%= render "form", comunicado: @comunicado %>
    </div>
</turbo-frame>
